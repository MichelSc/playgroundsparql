@prefix : <http://www.flandersmake.be/ontology/StartEvent#> .
@prefix mm: <http://www.flandersmake.be/ontology/mm#> .
@prefix st: <http://www.flandersmake.be/ontology/StartEvent#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.flandersmake.be/ontology/StartEvent> .

<http://www.flandersmake.be/ontology/StartEvent> rdf:type owl:Ontology .

#################################################################
#    Model for the StartEvent ontolgy
#################################################################

#################################################################
#    Classes
#################################################################

st:Phase rdf:type owl:Class .

st:Asset 
    rdf:type owl:Class ;
    rdfs:label "Assets" .

st:SampleA 
    rdf:type owl:Class ;
    rdfs:subClassOf st:Asset ;
    rdfs:comment "A subclass of Asset" .

st:SampleB 
    rdf:type owl:Class ;
    rdfs:subClassOf st:Asset ;
    rdfs:comment "Another subclass of Asset" , "Maybe we should consider making a class Sample" .

st:Event rdf:type owl:Class .

st:Step 
    rdf:type owl:Class ;
    rdfs:subClassOf st:Event .

st:BadStep 
    rdf:type owl:Class ;
    rdfs:subClassOf st:Step .

st:PlasmaStep 
    rdf:type owl:Class ;
    rdfs:subClassOf st:Step .

st:CuringStep 
    rdf:type owl:Class ;
    rdfs:subClassOf st:Step .



#################################################################
#    Properties
#################################################################

st:start 
    rdf:type owl:DatatypeProperty ;
    rdfs:subPropertyOf owl:topDataProperty ;
    rdfs:range xsd:dateTime .

st:temperature 
    rdf:type owl:DatatypeProperty ;
    rdfs:subPropertyOf owl:topDataProperty ;
    rdfs:range xsd:float .

st:assetBreakstress 
    rdf:type owl:DatatypeProperty ;
    rdfs:domain st:Asset ;
    rdfs:range xsd:float .

st:assetName 
    rdf:type owl:DatatypeProperty ;
    rdfs:subPropertyOf owl:topDataProperty ;
    rdfs:domain st:Asset ;
    rdfs:range xsd:string .

st:assetWeight 
    rdf:type owl:DatatypeProperty ;
    rdfs:domain st:Asset ;
    rdfs:range xsd:float .

st:assetTemperature 
    rdf:type owl:DatatypeProperty ;
    rdfs:subPropertyOf st:temperature ;
    rdfs:domain st:Asset .
					
st:eventSubject 
    rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain st:Event ;
    rdfs:range st:Asset .

st:eventPhase 
    rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain st:Event ;
    rdfs:range st:Phase .

st:eventStart 
    rdf:type owl:DatatypeProperty ;
    rdfs:subPropertyOf st:start ;
    rdfs:domain st:Event .

st:stepStart 
    rdf:type owl:DatatypeProperty ;
    rdfs:subPropertyOf st:eventStart ;
    rdfs:domain st:Step .

st:curingStepTemperature
	rdf:type owl:DatatypeProperty ;
    rdfs:subPropertyOf st:temperature ;
    rdfs:domain st:CuringStep .

#################################################################
#    Observable
#################################################################

st:Asset rdf:type mm:Observable.

st:DAssetBreakStress 
  mm:dimensionContext st:Asset;
  mm:dimensionProperty st:assetBreakstress. 

st:DAssetCuringTemperature
  mm:dimensionContext st:CAssetCuringStep;
  mm:dimensionProperty st:curingStepTemperature. 
       
st:CAssetCuringStep 
    mm:observableContextObservable st:Asset;
    mm:observableContextNavigationNavigationPath [  
        mm:navigationPathFirst [    
            mm:navigationPathElementDirect false;
            mm:navigationPathElementProperty st:eventSubject
        ];
        mm:navigationPathRest [
            mm:navigationPathFirst [    
                mm:navigationPathElementDowncast st:CuringStep
            ];
            mm:navigationPathRest rdf:nil
        ]
    ].

st:CBadStep 
    mm:observableContextObservable st:Event;
    mm:observableContextNavigationNavigationPath [  
        mm:navigationPathFirst [    
            mm:navigationPathElementDowncast st:BadStep
        ];
        mm:navigationPathRest rdf:nil
    ].

st:CAssetBadCuringStep 
    mm:observableContextObservable st:Asset;
    mm:observableContextNavigationNavigationPath [  
        mm:navigationPathFirst [    
            mm:navigationPathElementDirect false;
            mm:navigationPathElementProperty st:eventSubject
        ];
        mm:navigationPathRest [
            mm:navigationPathFirst [    
                mm:navigationPathElementDowncast st:BadStep
            ];
            mm:navigationPathRest rdf:nil
        ]
    ].

st:CAssetCuringStepPhase
    mm:observableContextObservable st:Asset;
    mm:observableContextNavigationNavigationPath [  
        mm:navigationPathFirst [    
            mm:navigationPathElementDirect false;
            mm:navigationPathElementProperty st:eventSubject
        ];
        mm:navigationPathRest [
            mm:navigationPathFirst [    
                mm:navigationPathElementDirect true;
                mm:navigationPathElementProperty st:eventPhase
            ];
            mm:navigationPathRest rdf:nil
        ]
    ].
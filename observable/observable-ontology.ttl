#################################################################
#    ObservableContext
#################################################################

# an ObservableContext provides a context to be observed in a contextual class
mm:ObservableContext 
    rdfs:subClassOf owl:ObjectProperty . # its elements are properties, associating the contextual class with its observable contexts

mm:observableContextObservable 
    rdf:type    rdf:Property ;
    rdfs:domain mm:ObservableContext ;
    rdfs:range  mm:Observable .

mm:ObservableContextNavigation
    rdfs:subClassOf mm:ObservableContext .

mm:observableContextNavigationNavigationPath 
    rdf:type    rdf:Property ;
    rdfs:domain mm:ObservableContextNavigation ;
    rdfs:range  mm:NavigationPath .

mm:NavigationPath 
    rdfs:subClassOf rdf:List .

mm:NavigationPathElement 
    rdf:type rdf:Class .

mm:navigationPathFirst 
    rdfs:subPropertyOf rdf:first;
    rdfs:domain mm:NavigationPath ;
    rdfs:range mm:NavigationPathElement .

mm:NavigationPathElementProperty 
    rdfs:subClassOf mm:NavigationPathElement .

mm:navigationPathElementProperty 
    rdf:type    rdf:Property;
    rdfs:domain mm:NavigationPathElementProperty ;
    rdfs:range  owl:ObjectProperty .

mm:navigationPathElementDirect 
    rdf:type    rdf:Property ;
    rdfs:domain mm:NavigationPathElementProperty ;
    rdfs:range  xsd:boolean .

mm:NavigationPathElementDowncast
    rdfs:subClassOf mm:NavigationPathElement .

mm:navigationPathElementDowncast
    rdf:type    rdf:Property ;
    rdfs:domain mm:NavigationPathElementDowncast ;
    rdfs:range  owl:Class .

#################################################################
#    Dimension
#################################################################

# a Dimension associates a value to an Observable via an ObservableContext
mm:Dimension rdfs:subClassOf owl:DatatypeProperty . # its elements are properties, associating the contextual class with its observable values

mm:dimensionContext  
    rdf:type rdf:Property;
    rdfs:domain mm:Dimension ;
    rdfs:range mm:ObservableContext .

mm:DimensionProperty rdfs:subClassOf mm:Dimension .

mm:dimensionProperty 
    rdf:type    rdf:Property ;
    rdfs:domain mm:DimensionProperty ;
    rdfs:range  owl:DatatypeProperty .

mm:DimensionDimension rdfs:subClassOf mm:Dimension .

mm:dimensionDimension 
    rdf:type    rdf:Property  ;
    rdfs:domain mm:DimensionDimension ;
    rdfs:range  mm:Dimension .

#################################################################
#    Observation
#################################################################

mm:Observation 
    rdf:type rdfs:Class .

mm:UnaryObservation 
    rdfs:subClassOf mm:Observation .

mm:BinaryObservation 
    rdfs:subClassOf  mm:Observation .

mm:observationArity 
    rdf:type rdf:Property ;
    rdfs:domain mm:Observation ;
    rdfs:range xsd:integer .

mm:unaryObservationDimension 
    rdf:type    rdf:Property ;
    rdfs:domain mm:UnaryObservation ;
    rdfs:range  mm:Dimension .

mm:binaryObservationDimension1 
    rdf:type    rdf:Property ;
    rdfs:domain mm:BinaryObservation ;
    rdfs:range  mm:Dimension .

mm:binaryObservationDimension2 
    rdf:type    rdf:Property ;
    rdfs:domain mm:BinaryObservation ;
    rdfs:range  mm:Dimension .

mm:observationGroupingDimensions 
    rdf:type    rdf:Property ;
    rdfs:domain mm:Observation ;
    rdfs:range  mm:Dimension .

#################################################################
#    Property/Influence TODO
#################################################################

mm:Property 
    rdf:type 
        owl:Class .

mm:dimensionProperties
    rdf:type 
        owl:ObjectProperty ;
    rdfs:subPropertyOf 
        owl:topObjectProperty ;
    rdfs:domain 
        mm:Dimension ;
    rdfs:range 
        mm:Property .

mm:Influence
    rdf:type 
        owl:Class .

mm:influenceProperties
    rdf:type 
        owl:ObjectProperty ;
    rdfs:subPropertyOf 
        owl:topObjectProperty ;
    rdfs:domain 
        mm:Influence ;
    rdfs:range 
        mm:Property .

mm:observedInfluence
    rdf:type 
        owl:ObjectProperty ;
    rdfs:subPropertyOf 
        owl:topObjectProperty ;
    rdfs:domain 
        mm:Observation ;
    rdfs:range 
        mm:Influence .



